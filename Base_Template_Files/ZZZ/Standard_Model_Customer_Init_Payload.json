      "ipsec-profiles": {
        "ipsec-profile": [
          {
            "shared": true,
            "crypto-profile": "EAS_CRYPTO_PROFILE",
            "ipsec-profile-name": "EAS_IPSEC_PROFILE",
            "transform-set": "EAS_IPSEC_TSET"
          }
        ]
      },
      "crypto-profiles": {
        "crypto-profile": [
          {
            "crypto-type": "IKEV1",
            "encryption": "AES128",
            "authentication": "SHA1",
            "allow-all": true,
            "crypto-profile-name": "EAS_CRYPTO_PROFILE",
            "auth-type": "pre-share",
            "policy-number": 3,
            "vrf": "INTERNET",
            "group": 15,
            "keyring": "EAS_CRYPTO_KEYRING",
            "pre-shared-key": "{{EAS_PROFILE_PRE_SHARED_KEY}}"
          }
        ]
      },
      "transform-sets": {
        "transform-set": [
          {
            "encryption": "esp-aes",
            "authentication": "esp-sha-hmac",
            "transform-set-name": "EAS_IPSEC_TSET",
            "mode": "transport"
          }
        ]
      }
    },
    "dmvpn-tunnel-profiles": {
      "dmvpn-tunnel-profile": [
        {
          "mtu": 1400,
          "tunnel-key": "{{EAS_TUNNEL_INTERFACE_KEY}}",
          "nhrp-shortcut": true,
          "tunnel-prefix": "{{CUSTOMER_SFDC_CUID}}_EAS_{{EAS_TUNNEL_IP_SUBNET}}",
          "tcp-adjust-mss": 1360,
          "nhrp-reg-no-uniq": false,
          "nhrp-authentication-key": "{{EAS_TUNNEL_NHRP_AUTH_KEY}}",
          "nhrp-redirect": false,
          "nhrp-holdtime": 600,
          "wan-public-ip": "{{EAS_GATEWAY_PUBLIC_IP}}",
          "tunnel-id": "{{EAS_TUNNEL_INTERFACE_ID}}",
          "name": "{{EAS_PROFILE_NAME}}_DMVPN_PROFILE",
          "ipsec-profile": "EAS_IPSEC_PROFILE",
          "nhrp-nw-id": "{{EAS_TUNNEL_INTERFACE_NETWORK_ID}}",
          "no-nhrp-route-watch": false,
          "wan-tunnel-ip": "{{EAS_GATEWAY_TUNNEL_INTERFACE_IP}}"
        }
      ]
    },
    {% endif %}
